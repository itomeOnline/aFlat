							.home_main__table
								.catalog_filters__block
									p.catalog_filters__caption Тип недвижимости
									.catalog_filters__select.select(data-dropdown="", data-dropdown-name="propertyType", data-dropdown-placeholder="Тип недвижимости", data-dropdown-readonly="true", data-dropdown-options='[{"key":"Дом","value":"Дом"},{"key":"Дача","value":"Дача"},{"key":"Нежилое","value":"Нежилое"}]') 

							.home_main__buttons 
								button.home_main__btn.btn.btn--small.btn--filled
